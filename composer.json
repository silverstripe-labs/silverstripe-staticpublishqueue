{
    "name": "silverstripe/staticpublishqueue",
    "description": "Static publishing queue to create static versions of pages for enhanced performance and security",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "static",
        "html",
        "security",
        "performance",
        "static-publishing",
        "caching",
        "cache",
        "static-caching",
        "static-cache",
        "queue",
        "publishing"
    ],
    "require": {
        "ext-zlib": "*",
        "silverstripe/framework": "^4.0.2",
        "silverstripe/cms": "^4",
        "silverstripe/config": "^1",
        "symbiote/silverstripe-queuedjobs": "^4.0.6",
        "silverstripe/versioned": "^1.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\StaticPublishQueue\\": "src/",
            "SilverStripe\\StaticPublishQueue\\Test\\": "tests/php/"
        },
        "files": [
            "includes/functions.php"
        ]
    },
    "include-path": [
        "includes/"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "scripts": {
        "lint": "phpcs -s src/ tests/php/",
        "lint-clean": "phpcbf src/ tests/php/"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
